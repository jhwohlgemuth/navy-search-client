{
  "name": "navy-search-client",
  "version": "1.0.0",
  "author": "Jason Wohlgemuth",
  "license": "MIT",
  "description": "Navy Search client built with Marionette.js",
  "main": "./app/main.js",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/some-user/navy-search-client.git"
  },
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone.marionette": "^3.3.1",
    "backbone.radio": "^2.0.0",
    "bluebird": "^3.5.0",
    "handlebars": "^4.0.8",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "perfect-scrollbar": "^0.7.0",
    "requirejs": "^2.3.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "autoprefixer": "^9.4.7",
    "browser-sync": "^2.18.6",
    "browserify": "^14.3.0",
    "browserify-shim": "^3.8.14",
    "config": "^1.26.1",
    "cssnano": "^3.9.1",
    "deamdify": "^0.3.0",
    "eslint-config-omaha-prime-grade": "^4.0.0",
    "eslint-plugin-backbone": "^2.0.1",
    "fs-promise": "^2.0.3",
    "globby": "^6.0.0",
    "grunt": "^1.0.0",
    "grunt-a11y": "^0.1.4",
    "grunt-accessibility": "^5.0.0",
    "grunt-benchmark": "^1.0.0",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "^1.0.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-csslint": "^2.0.0",
    "grunt-contrib-handlebars": "^1.0.0",
    "grunt-contrib-htmlmin": "^2.3.0",
    "grunt-contrib-imagemin": "^2.0.0",
    "grunt-contrib-requirejs": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^3.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-express": "^1.4.1",
    "grunt-htmlhint-plus": "^0.4.3",
    "grunt-jsdoc": "^2.1.0",
    "grunt-jsinspect": "^2.0.0",
    "grunt-jsonlint": "^1.0.4",
    "grunt-karma": "^2.0.0",
    "grunt-karma-coveralls": "^2.5.4",
    "grunt-open": "^0.2.3",
    "grunt-parallel": "^0.5.0",
    "grunt-plato": "^1.3.0",
    "grunt-postcss": "^0.8.0",
    "grunt-replace": "^1.0.1",
    "jasmine-core": "^2.6.1",
    "jsinspect": "^0.12.4",
    "json-server": "^0.10.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-requirejs": "^1.1.0",
    "karma-spec-reporter": "0.0.31",
    "load-grunt-tasks": "^3.1.0",
    "mdcss": "^1.5.2",
    "mdcss-theme-github": "^2.4.2",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-safe-parser": "^2.0.0",
    "sinon": "^2.2.0",
    "time-grunt": "^1.1.0"
  },
  "scripts": {
    "presymlink": "if [ -L `pwd`/app/assets ]; then rm `pwd`/app/assets ; fi",
    "symlink": "ln -s `pwd`/assets `pwd`/app/assets",
    "test": "grunt test",
    "test:ci": "npm test && grunt coveralls",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "rest-api": "json-server --port 8080 --watch test/data/db.json",
    "inspect": "grunt jsinspect:app",
    "build": "grunt build",
    "predemo": "npm run build",
    "demo": "grunt open:demo express:demo",
    "prestart": "nohup npm run rest-api &",
    "start": "npm run symlink && grunt serve",
    "predeploy": "npm run build",
    "deploy": "cp -a dist/. ../navy-search/web/client/."
  },
  "browserify": {
    "transform": [
      "deamdify",
      "aliasify"
    ]
  },
  "aliasify": {
    "aliases": {
      "app": "././app/app",
      "router": "././app/router",
      "templates": "././app/templates"
    },
    "replacements": {
      "models/(\\w+)": "././app/models/$1",
      "views/(\\w+)": "././app/views/$1",
      "controllers/(\\w+)": "././app/controllers/$1",
      "plugins/(\\w+)": "././app/plugins/$1"
    }
  }
}
